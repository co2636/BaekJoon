package 일차원배열;

import java.util.Scanner;

public class num2562 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int arr[] = new int[9];
		
		for(int i=1;i<=9;i++) {
			
			arr[i] = sc.nextInt();
		}
		int max = 0;
		int count = 0;
		for(int num : arr) {
			if(num > max) {
				max = num;
				count++;
			}
			
		}
		
	}

}
