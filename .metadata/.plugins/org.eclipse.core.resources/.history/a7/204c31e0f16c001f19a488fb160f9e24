 package 일차원배열;

import java.util.Scanner;

public class num10807 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);

		int N = sc.nextInt();
		int []arr = new int[N];
		
		for(int i = 0; i < N; i++) {
			arr[i] = sc.nextInt();
		
		}
		int count = 0;
		int v = sc.nextInt();
		for(int num : arr) {
			if(v == num) {
				count++;
			}
		}
		
	}

}
